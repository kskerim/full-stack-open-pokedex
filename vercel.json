{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "buildCommand": "npm run build",
          "outputDirectory": "dist"
        }
      }
    ],
    "routes": [
      { "src": "/bundle.js", "dest": "/bundle.js" },
      { "handle": "filesystem" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  } 